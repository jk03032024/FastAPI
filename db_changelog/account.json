                    }
                  },
                  {
                    "column": {
                      "name": "zip_code",
                      "type": "varchar(255)"
                    }
                  },
                  {
                    "column": {
                      "name": "contact_name",
                      "type": "varchar(255)",
                      "constraints": {
                          "nullable": false
                      }
                    }
                  },
                  {
                    "column": {
                      "name": "contact_email",
                      "type": "varchar(255)",
                      "constraints": {
