                      "type": "boolean",
                      "defaultValueBoolean":"false"
                    }
                  },
                  {
                    "column": {
                      "name": "is_self_registration_allowed",
                      "type": "boolean",
                      "defaultValueBoolean":"false"
                    }
                  },
                  {
                    "column": {
                      "name": "created_by",
                      "type": "varchar(255)",
                      "defaultValueComputed": "current_user",
                      "constraints": {
                          "nullable": false
                      }
                    }
                  },
                  {
                    "column": {
                      "name": "updated_by",
                      "type": "varchar(255)",
