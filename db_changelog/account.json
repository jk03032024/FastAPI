          "changes": [
            {
              "createTable": {
                "columns": [
                  {
                    "column": {
                      "name": "id",
                      "type": "UUID",
                      "defaultValueComputed": "uuid_generate_v4()",
                      "constraints":{
                        "primaryKey":true
                      }
                      
                    }
                  },
                  {
                    "column": {
                      "name": "name",
                      "type": "varchar(255)",
                      "constraints": {
                          "nullable": false
                      }
                    }
                  },
                  {
                    "column": {
                      "name": "parent_account_id",
                      "type": "varchar(255)",
                      "constraints": {
                          "nullable": false
                      }
                    }
                  },
                  {
                    "column": {
                      "name": "category",
                      "type": "varchar(255)",
                      "constraints": {
                          "nullable": false
                      }
                    }
                  },
                  {
                    "column": {
                      "name": "type",
                      "type": "varchar(255)",
                      "constraints": {
                          "nullable": false
                      }
